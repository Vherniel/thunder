{
  "name": "thunder",
  "version": "1.0.0",
  "description": "The open-source code of Arellano University's online merchandise store web-app.",
  "main": "index.js",
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "npm": "^6.6.0",
    "concurrently": "^4.1.0",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.16.1"
  },
  "scripts": {
    "all": "concurrently \"npm run pug\" \"npm run sass\" \"npm run pug1\"",
    "pug": "pug src/pug/index.pug -w -P -o build",
    "pug1": "pug src/pug/product/product-page.pug -w -P -o build",
    "sass": "sass --watch src/scss/index.scss build/css/index.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vherniel/thunder.git"
  },
  "keywords": [
    "Merchandise",
    "Store",
    "Merch",
    "Arellano",
    "University",
    "AU",
    "Chiefs"
  ],
  "author": "Vherniel L. Lebis, Jeremiah Felipe, Kyle Gabriel Calolot, Goy Culdora, Jhomer Ventura Castro, Jane Gultiano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vherniel/thunder/issues"
  },
  "homepage": "https://github.com/Vherniel/thunder#readme"
}
